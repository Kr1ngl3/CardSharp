<UserControl
    x:Class="CardSharp.Views.GameView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:CardSharp.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:m="clr-namespace:CardSharp.Models"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="clr-namespace:CardSharp.ViewModels"
    d:DesignHeight="450"
    d:DesignWidth="1200"
    x:DataType="vm:GameViewModel"
    mc:Ignorable="d">
    <c:Table Width="1920" Height="1020">
        <Button Command="{CompiledBinding Move}" HotKey="P">Click</Button>
    </c:Table>
</UserControl>
